<form id="ucus-guzergahi"  method="post">
    <div id="form">
        <div id="neredenDiv" class="form-div">
            <label id="neredenLabel">@languageService.GetKey("FormPartial.Nereden")</label>
            <input class="form-control" id="nereden" type="text" />
        </div>
        <div class="form-div">
            <label>@languageService.GetKey("FormPartial.Nereye")</label>
            <input class="form-control" id="nereye" type="text"/>
        </div>
        <div class="form-div">
            <label class="kucuk-label">@languageService.GetKey("FormPartial.Gidis")</label>
            <input class="form-control kucuk-label-input" type="date" placeholder=""/>
        </div>
        @if(false){
            <div class="form-div">
                <label class="kucuk-label">@languageService.GetKey("FormPartial.Donus")</label>
                <input class="form-control kucuk-label-input" type="date" placeholder=""/>
            </div>
        }
        <div class="form-div">
            <label class="kucuk-label">@languageService.GetKey("FormPartial.Yolcu")</label>
            <input class="form-control kucuk-label-input" type="number" />
        </div>
        <div id="form-button" class="form-div">
            <input class="form-control btn-success" type="button" value="@languageService.GetKey("FormPartial.Button")"/>
        </div>
    </div>
</form>

<script src="../Views/Shared/Partials/FormPartial.cshtml.js"></script>
